{
  "id": "upload",
  "type": "object",
  "required": [ "md5Hash", "contentLength", "name" ],
  "properties": {
    "contentType": {
      "type": "string"
    },
    "md5Hash": {
      "type": "string",
      "format": "^[0-9A-Fa-f]{32}$"
    },
    "contentLength": {
      "type": "integer",
      "minimum": 1,
      "maximum": 2000000000
    },
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 150
    }
  }
}