{
  "name": "ms-files",
  "version": "0.1.1",
  "description": "Core of microservice",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators ./src; fi",
    "prepublish": "npm run compile",
    "pretest": "npm rebuild",
    "test": "BLUEBIRD_DEBUG=1 ./node_modules/.bin/mocha --require ./test/babelhook.js --bail -R spec",
    "test-local": "NODE_ENV=development ./node_modules/.bin/mocha --require ./test/babelhook.js",
    "start": "node ./bin/mservice.js | ./node_modules/.bin/bunyan -o short"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/ms-files.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-files/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-files#readme",
  "dependencies": {
    "bluebird": "^3.0.5",
    "bunyan": "^1.5.1",
    "common-errors": "^0.5.3",
    "ioredis": "^1.11.0",
    "lodash": "^3.10.1",
    "ms-amqp-conf": "^0.2.0",
    "ms-amqp-transport": "^0.2.7",
    "ms-amqp-validation": "^0.4.1",
    "ms-files-gce": "^0.1.0",
    "mservice": "^0.10.0"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.4",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.1.0",
    "eslint-plugin-react": "^3.9.0",
    "json": "^9.0.3",
    "mocha": "^2.3.3",
    "sinon": "^1.17.2"
  }
}
